<div class="row">
  <div class="col-xm-9 col-sm-9 col-md-9 col-lg-9">
    <div class="text-header">
      Create an account
    </div>
    <div class="h-seperator"></div>
    <div class="clear"></div>
    <div class="clear"></div>
    <div class="row-fluid sign_up_block">
      <% if params[:account_type].blank?%>
        <%= render partial: 'account_type_list' %>
      <% else %>
        <%= render :partial => "signup_page", locals: {user_type: params[:account_type]} %>
      <% end %>
    </div>
  </div>
  <div class="col-xm-3 col-sm-3 col-md-3 col-lg-3">
    <%= render :partial => "shared/side_bar" %>
  </div>
</div>
