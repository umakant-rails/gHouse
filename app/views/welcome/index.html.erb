<div class="row menu-row">
  <div class="col-xm-9 col-sm-9 col-md-9 col-lg-9">
    <%= render :partial => "welcome/home_carousel" %>
  </div>
  <div class="col-xm-3 col-sm-3 col-md-3 col-lg-3">
    <div class="row">
      <div class="col-xm-12 col-sm-12 col-md-12 col-lg-12">
        <%= render :partial => "shared/property_summary" %>
      </div>
    </div>
  </div>
</div>
<div class="clear"></div>
<div class="clear"></div>
<div class="clear"></div>
<div class="properties_list">
  <div class="row menu-row">
    <div class="col-xm-9 col-sm-9 col-md-9 col-lg-9">
      <div class="properties-header">
        New Properties List
      </div>
      <div class="h-seperator"></div>
      <% 9.times do | index | %>
        <% if (index % 3 == 0) %>
          <div class="clear"></div>
          <div class="clear"></div>
          <div class="row property-row" style="margin: 0px;">
        <% end %>
        <div class="border_prods col-xs-4">
          <%= render :partial => "shared/property_summary" %>
        </div>
        <% if (index % 3 == 2) %>
         </div>
        <% end %>
      <% end %>
    </div>
    <div class="col-xm-3 col-sm-3 col-md-3 col-lg-3">
      <div class="properties-header">
        Offers
      </div>
      <div class="h-seperator"></div>
      <div class="clear"></div>
      <div class="clear"></div>
      <div class="row">
        <div class="col-xm-12 col-sm-12 col-md-12 col-lg-12">
          <%= render :partial => "shared/property_summary" %>
        </div>
      </div>
    </div>
  </div> 
</div>
